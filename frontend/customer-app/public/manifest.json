  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.garbaking.customer",
      "id": "com.garbaking.customer"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/garbaking/id123456789"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+garbaking",
      "url": "/order?id=%s"
    }
  ],
  "share_target": {
    "action": "/share-order",
    "method": "POST",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/import-order",
      "accept": {
        "application/json": [".json"]
      }
    }
  ]
}