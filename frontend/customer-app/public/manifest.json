    {
      "name": "Voir le menu",
      "short_name": "Menu",
      "description": "Découvrir notre carte complète",
      "url": "/menu",
      "icons": [
        {
          "src": "/icons/shortcut-menu.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Mon panier",
      "short_name": "Panier",
      "description": "Voir mes articles en cours",
      "url": "/cart",
      "icons": [
        {
          "src": "/icons/shortcut-cart.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Suivre ma commande",
      "short_name": "Suivi",
      "description": "Voir le statut de ma commande",
      "url": "/order-status",
